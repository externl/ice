<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <MSBuildCachePackageName>Microsoft.MSBuildCache.Local</MSBuildCachePackageName>
    <MSBuildCachePackageVersion>0.1.271-preview</MSBuildCachePackageVersion>
    <MSBuildCachePackageRoot>
      ..\msbuild\packages\$(MSBuildCachePackageName).$(MSBuildCachePackageVersion)</MSBuildCachePackageRoot>
  </PropertyGroup>
  <Import Project="$(MSBuildCachePackageRoot)\build\$(MSBuildCachePackageName).props"
    Condition="Exists('$(MSBuildCachePackageRoot)\build\$(MSBuildCachePackageName).props')" />
</Project>
